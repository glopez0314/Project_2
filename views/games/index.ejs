<%- include('../partials/header') %>

<table>
  <thread>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th></th>
    </tr>
  </thread>
  <tbody>
    <% games.forEach(g => { %>
    <tr>
      <td><%= g.title %></td>
      <td><%= g.releaseYear %></td>
      <td><%= g.genre %></td>
      <td><a href="/games/<%= g._id %>">DETAILS</a></td>
    </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>
